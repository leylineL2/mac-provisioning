[alias]
	cancel  = reset --soft HEAD^
	current = !git rev-parse --abbrev-ref HEAD
	pull-f  = !git fetch && git reset --hard origin/$(git current)
	unstage = reset HEAD --
[credential]
	helper = !aws --region ap-northeast-1  codecommit credential-helper $@
	UseHttpPath = true
[user]
	name = leyla
	email = leyla@local
